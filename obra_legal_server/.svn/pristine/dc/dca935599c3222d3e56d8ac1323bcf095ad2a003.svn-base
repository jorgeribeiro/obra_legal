package br.gov.ma.tce.obralegal.server.modalidadeprofissional;

public enum ModalidadeProfissional {

	AGRIMENSURA("AGRIMENSURA"), AGRONOMIA("AGRONOMIA"), ARQUITETURA("ARQUITETURA"), CIVIL("CIVIL"),
	ELETRICISTA("ELETRICISTA"), ESPECIALIZACAO("ESPECIALIZACAO"), GEOLOGIA_MINAS("GEOLOGIA E MINAS"),
	MECANICA_METALURGIA("MECÂNICA E METALURGIA"), QUIMICA("QUÍMICA");

	private String descricao;

	private ModalidadeProfissional(String descricao) {
		this.descricao = descricao;
	}

	public String getDescricao() {
		return descricao;
	}
}
