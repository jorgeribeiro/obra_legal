package br.gov.ma.tce.obralegal.server.tipodocumento;

public enum TipoDocumento {

	ART("ART"), DOCUMENTO_PROFISSIONAL("DOCUMENTO PROFISSIONAL"), PROJETO("PROJETO"), SITUACAO("SITUAÇÃO"),
	OUTROS("OUTROS");

	private String descricao;

	private TipoDocumento(String descricao) {
		this.descricao = descricao;
	}

	public String getDescricao() {
		return descricao;
	}
}
